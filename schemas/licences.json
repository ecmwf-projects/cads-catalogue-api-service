{
  "$id": "/schemas/licences",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CADS licences vocabularies",
  "type": "object",
    "properties": {
      "licences": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "label": {
              "type": "string"
            },
            "revision": {
              "type": "number"
            }
          },
          "required": [
            "id",
            "label",
            "revision"
          ]
        }
      }
    },
    "required": [
      "licences"
    ]
  }